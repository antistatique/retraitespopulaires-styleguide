<div class="footer hidden-print">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 text-center">
        <h2>{% trans %}Retrouvez-nous sur les réseaux sociaux{% endtrans %}</h2>
        <p>{% trans %}Suivez-nous en temps réel sur Twitter. Facebook et vos plateformes préférées!{% endtrans %}</p>
        <div class="spacer spacer-xs"></div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12 text-center social-list">
        <div class="button-bubble button-bubble-social inline-block button-bubble-outline text-center">
          <div class="spacer spacer-xs"></div>
          <div class="bubble-icon">
            <a aria-label="Cliquez pour accéder au compte Twitter de Retraites Populaires" href="{{ variables.contact.twitter }}" target="blank"><i class="retraitespopulaires-icon retraitespopulaires-icon-3x retraitespopulaires-icon-twitter retraitespopulaires-icon-color" aria-hidden="true"></i></a>
          </div>
        </div>

        <div class="button-bubble button-bubble-social inline-block button-bubble-outline text-center">
          <div class="spacer spacer-xs"></div>
          <div class="bubble-icon">
            <a aria-label="Cliquez pour accéder au compte Facebook de Retraites Populaires" href="{{ variables.contact.facebook }}" target="blank"><i class="retraitespopulaires-icon retraitespopulaires-icon-3x retraitespopulaires-icon-facebook retraitespopulaires-icon-color" aria-hidden="true"></i></a>
          </div>
        </div>

        <div class="button-bubble button-bubble-social inline-block button-bubble-outline text-center">
          <div class="spacer spacer-xs"></div>
          <div class="bubble-icon">
            <a aria-label="Cliquez pour accéder au compte YouTube de Retraites Populaires" href="{{ variables.contact.youtube }}" target="blank"><i class="retraitespopulaires-icon retraitespopulaires-icon-3x retraitespopulaires-icon-youtube retraitespopulaires-icon-color" aria-hidden="true"></i></a>
          </div>
        </div>

        <div class="button-bubble button-bubble-social inline-block button-bubble-outline text-center">
          <div class="spacer spacer-xs"></div>
          <div class="bubble-icon">
            <a aria-label="Cliquez pour accéder au compte Instagram de Retraites Populaires" href="{{ variables.contact.instagram }}" target="blank"><i class="retraitespopulaires-icon retraitespopulaires-icon-3x retraitespopulaires-icon-instagram retraitespopulaires-icon-color" aria-hidden="true"></i></a>
          </div>
        </div>

        <div class="button-bubble button-bubble-social inline-block button-bubble-outline text-center">
          <div class="spacer spacer-xs"></div>
          <div class="bubble-icon">
            <a aria-label="Cliquez pour accéder au compte LinkedIn de Retraites Populaires" href="{{ variables.contact.linkedin }}" target="blank"><i class="retraitespopulaires-icon retraitespopulaires-icon-3x retraitespopulaires-icon-linkedin retraitespopulaires-icon-color" aria-hidden="true"></i></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="spacer"></div>

  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-5 col-md-offset-1">
        <div class="spacer spacer-md visible-xs visible-sm"></div>
        <p>
          {% set link = path('entity.node.canonical', {'node': 179}) %}
          {% trans %}Recevez <a href="{{ link }}" class="pretty-link text-italic"><i>Savoir-faire</i></a>, le bulletin des spécialistes de Retraites&nbsp;Populaires, trois fois par année via e-mail:{% endtrans %}</p>
        {{ load_form('rp_newsletter', 'SubscribeForm') }}
      </div>

      <div class="col-xs-12 col-md-5 col-md-offset-1">
        <div class="spacer spacer-md visible-xs visible-sm"></div>
        <p>{% trans %}Contactez-nous{% endtrans %}</p>

        <ul class="nav nav-tabs" role="tablist">
          <li role="presentation" class="active"><a href="#lausanne" aria-controls="lausanne" role="tab" data-toggle="tab">{% trans %}Lausanne (siège){% endtrans %}</a></li>
          <li role="presentation"><a href="#yverdon" aria-controls="yverdon" role="tab" data-toggle="tab">{% trans %}Yverdon-les-Bains{% endtrans %}</a></li>
          <li role="presentation"><a href="#nyon" aria-controls="nyon" role="tab" data-toggle="tab">{% trans %}Nyon{% endtrans %}</a></li>
        </ul>

        <div class="spacer spacer-sm"></div>
        <div class="row">
          <div class="col-xs-12">
            <div class="tab-content">
              <div role="tabpanel" class="tab-pane active" id="lausanne">
                {% set tel = variables.contact.phone_lausanne|replace({' ': ''}) %}
                <a href="tel:{{ tel }}" class="text-large d-block text-no-wrap">{{ variables.contact.phone_lausanne }}</a>
                <a href="mailto:{{ variables.contact.email }}" class="text-large d-block">{{ variables.contact.email }}</a>
              </div>
              <div role="tabpanel" class="tab-pane" id="yverdon">
                {% set tel = variables.contact.phone_yverdon|replace({' ': ''}) %}
                <a href="tel:{{ tel }}" class="text-large d-block text-no-wrap">{{ variables.contact.phone_yverdon }}</a>
                <a href="mailto:{{ variables.contact.email_yverdon }}" class="text-large d-block">{{ variables.contact.email_yverdon }}</a>
              </div>
              <div role="tabpanel" class="tab-pane" id="nyon">
                {% set tel = variables.contact.phone_nyon|replace({' ': ''}) %}
                <a href="tel:{{ tel }}" class="text-large d-block text-no-wrap">{{ variables.contact.phone_nyon }}</a>
                <a href="mailto:{{ variables.contact.email_nyon }}" class="text-large d-block">{{ variables.contact.email_nyon }}</a>
              </div>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>

  <div class="spacer spacer-xs"></div>

  <div class="organic-lines"></div>

  <div class="spacer"></div>

  <div class="container">
    <div class="row">
      <div class="col-xs-12 text-center">
        <ul class="menu-footer unordered-list list-inline list-unstyle-first">
          {% for item in variables.footer_menu %}
          <li>
            {% set target = item.link.options.external ? '_blank' %}
            <a class="pretty-link" href="{{ item.link.urlObject }}" target="{{ target }}">
              {{ item.link.title }}
            </a>
          </li>
          {% endfor %}
        </ul>
      </div>
    </div>
  </div>
    <!-- BEGIN Optimal Workshop Intercept Snippet -->
    <div id='owInviteSnippet'
         style='position:fixed;right:20px;bottom:20px;width:280px;padding:20px;margin:0;border-radius:6px;background:#333333;color:#EEEEEE;text-align:left;z-index:2200000000;opacity:0;transition:opacity 500ms;-webkit-transition:opacity 500ms;display:none;'>
        <div id='owInviteMessage' style='padding:0;margin:0 0 20px 0;font-size:16px;'>Aidez nous à améliorer notre site,
            participez à un test de 5 minutes
        </div>
        <a id='owInviteOk'
           href='https://ratio.optimalworkshop.com/treejack/45w870s6-0-0?tag=intercept&utm_medium=intercept'
           target='_blank'
           style='color:#CBB900;font-size:16px;font-weight:bold;text-decoration:underline;'>Commencer</a>
        <a id='owInviteCancel' href='javascript:void(0)' onclick='this.parentNode.style.display="none";'
                style='color:#EEEEEE;font-size:14px;text-decoration:underline;float:right;'>Non merci</a>
    </div>
    <script>var owOldOnload = window.onload;
        window.onload = function () {
            if (owOldOnload && owOldOnload(), -1 == document.cookie.indexOf('owInvite')) {
                var o = new XMLHttpRequest;
                o.onloadend = function () {
                    try {
                        var o = document.getElementById('owInviteSnippet');
                        this.response && JSON.parse(this.response).active === !0 && (document.cookie = 'owInvite=Done', setTimeout(function () {
                            o.style.display = 'block', o.style.opacity = 1
                        }, 2e3))
                    } catch (e) {
                    }
                }, o.open('POST', 'https://www.optimalworkshop.com/survey_status/treejack/0y52ziqf/active'), o.send()
            }
        };
    </script><!-- END Optimal Workshop snippet -->
  <div class="spacer"></div>
</div>
