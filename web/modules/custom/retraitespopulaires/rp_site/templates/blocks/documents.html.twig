<div class="{{ theme_hook_original }}">
  Documents block
  <ul>
   {% for doc in variables.documents %}
    <li>
      <a href="{{ path('rp_site.documents.download', {'node': doc.nid.value}) }}">{{ doc.title.value }}</a>
    </li>
   {% endfor %}
  </ul>

  <a href="{{ variables.collection.link }}">{% trans %}Accéder à tous les formulaires relatifs à {{ variables.collection.name }}{% endtrans %}</a>
</div>
