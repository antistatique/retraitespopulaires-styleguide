<div class="card-attachement card-attachement-{{ variables.theme }} shadow shadow-down">
  <div class="card-title h2">{% trans %}Questions fréquentes{% endtrans %}</div>
  <ul class="card-content list-spaced list-unstyled">
    {% for faq in variables.links %}
    <li>
      <a class="text-bold pretty-link h5" href="#">{{ faq.title.value }}</a>
    </li>
    {% endfor %}
  </ul>
  <div class="card-footer">
    <div class="media">
      <div class="media-left">
        <a href="#" class="retraitespopulaires-icon retraitespopulaires-icon-placeholder"></a>
      </div>
      <div class="media-body">
        <a class="text-bold pretty-link h5" href="{{ variables.collection.link }}">{% trans %}Accéder à toutes les questions relatives à {{ variables.collection.name }}{% endtrans %}</a>
      </div>
    </div>
  </div>
</div>
